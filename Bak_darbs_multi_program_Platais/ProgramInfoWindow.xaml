<Window x:Class="Bak_darbs_multi_program_Platais.ProgramInfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bak_darbs_multi_program_Platais"
        mc:Ignorable="d"
        Title="Program Info" Height="450" Width="800">
    <StackPanel Margin="10">
        <TextBlock Text="Enter Program Name" Margin="5"/>
        <TextBox x:Name="ProgramNameTextBox"  Width="200" Margin="5"/>
        <TextBlock Text="Enter Program Path" Margin="5"/>
        <TextBox x:Name="ProgramPathTextBox"  Width="200" Margin="5"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="5" Width="100" Click="SubmitButton_Click"/>



    </StackPanel>
</Window>
